<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_report_medical_consultation" model="ir.actions.report">
        <field name="name">Consultation report</field>
        <field name="model">medical.consultation</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">medical_center.report_medical_consultation</field>
        <field name="report_file">medical_center.report_medical_consultation</field>
        <field
            name="print_report_name"
        >'Consultation - %s - %s' % (object.name, object.date_consultation)</field>
        <field name="binding_model_id" ref="model_medical_consultation" />
        <field name="binding_type">report</field>
    </record>
</odoo>
